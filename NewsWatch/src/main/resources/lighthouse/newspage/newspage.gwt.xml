<module rename-to="com.lighthouse.newspage.newspage">
	<source path="client" />
	<inherits name="com.google.gwt.user.User" />
	<inherits name='com.google.gwt.user.theme.standard.Standard'/>
	<inherits name='com.lighthouse.login.user.lhuser' />
	<inherits name='com.newscenter.NewsCenter' /> 
	<inherits name='com.lighthouse.comment.comment' />
	<inherits name='com.lighthouse.main.lhmain' />
	
	<entry-point class='com.lighthouse.newspage.client.NewsItemHome' />

	<servlet path='/comments' class='com.lighthouse.comment.server.ItemCommentServiceImpl' />
</module>